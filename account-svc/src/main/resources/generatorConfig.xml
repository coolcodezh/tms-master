<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <classPathEntry location="mysql-connector-java-8.0.21.jar"/>

    <!--<context id="my" targetRuntime="MyBatis3">-->
    <context id="my" targetRuntime="MyBatis3">

        <!--是否去除注释-->
        <commentGenerator>
            <property name="suppressDate" value="false"/>
        </commentGenerator>
        <!-- 连接信息 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/tms?serverTimezone=UTC"
                        userId="root"
                        password="123456"
        >
            <property name="nullCatalogMeansCurrent"  value="true" />
            <!--生成update和delete-->
            <property name="useInformationSchema" value="true"/>
        </jdbcConnection>

        <!-- 最终生的java bean类的存储路径 以及包名 -->
        <javaModelGenerator targetPackage="vip.coolcode.account.model"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 配置最终生成的映射文件的位置 -->
        <sqlMapGenerator targetPackage="vip.coolcode.account.mapper"
                         targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!--接口生成的位置-->
        <javaClientGenerator targetPackage="vip.coolcode.account.mapper"
                             type="XMLMAPPER" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>



        <!--指定数据库表-->
        <!--<table tableName="city"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
               enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="true"
               enableDeleteByPrimaryKey="true"
                ></table>-->

        <!--指定数据库表-->
        <table tableName="account"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
               enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="true"
               enableDeleteByPrimaryKey="true"
        ></table>
    </context>


</generatorConfiguration>


